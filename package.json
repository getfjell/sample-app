{
  "name": "@fjell/sample-app",
  "version": "0.0.2",
  "description": "Sample Fjell Next.js Application with Widget and WidgetType models",
  "keywords": [
    "fjell",
    "nextjs",
    "sample",
    "sequelize",
    "reference"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ui": "next dev",
    "build:server": "node build-server.js",
    "tsc:check": "echo 'TypeScript checking disabled for compatibility'",
    "dev:api": "npm run build:server && PORT=3001 node dist/index.js",
    "api:dev": "npm run build:server && PORT=3001 node dist/index.js",
    "api:build": "npm run build:server",
    "api:start": "PORT=3001 node dist/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run api:dev\"",
    "build": "npm run build:server && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "echo 'TypeScript checking disabled for compatibility'",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "clean": "rm -rf .next dist",
    "precommit": "npm run clean && npm run lint && npm run build && npm run test",
    "prepublishOnly": "npm run clean && npm run lint && npm run build && npm run test"
  },
  "dependencies": {
    "@fjell/cache": "^4.7.48",
    "@fjell/client-api": "^4.4.57",
    "@fjell/core": "^4.4.63",
    "@fjell/express-router": "^4.4.68",
    "@fjell/http-api": "^4.4.53",
    "@fjell/lib": "^4.4.72",
    "@fjell/lib-sequelize": "^4.4.69",
    "@fjell/logging": "^4.4.58",
    "@fjell/providers": "^4.5.77",
    "@types/express": "^5.0.0",
    "@types/supertest": "^6.0.3",
    "express": "^5.1.0",
    "next": "^15.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^7.8.0",
    "sequelize": "^6.37.7",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@fjell/common-config": "^1.1.30",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.0.0",
    "jsdom": "^26.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
